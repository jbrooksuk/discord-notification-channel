{
    "name": "jbrooksuk/discord-notification-channel",
    "description": "Laravel notification channel for Discord",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "James Brooks",
            "email": "james@alt-three.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/notifications": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "JamesBrooks\\Discord\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
  "extra": {
    "laravel": {
      "providers": [
        "JamesBrooks\\Discord\\DiscordChannelServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
